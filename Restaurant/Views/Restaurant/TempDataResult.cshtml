@model RestaurantEntity.RestaurantFields

@{
    ViewBag.Title = "TempDataResult";
}
@Html.ActionLink("Add restaurant details","Create")
<h2>TempDataResult</h2>
<table border="1">
    <tr>
        <td>
            RestaurantName
        </td>
        <td>
            RestaurantType
        </td>
        <td>
            Location
        </td>
    </tr>
    @foreach (RestaurantEntity.RestaurantFields items in (List<RestaurantEntity.RestaurantFields>)TempData["RestaurantDetails"])
    {
        <tr>
            <td>
                @items.RestaurantName
            </td>
            <td>
                @items.RestaurantType
            </td>
            <td>
                @items.Location
            </td>
            <td>
                @Html.ActionLink("Edit","Edit",new { id=items.RestaurantId})
                @Html.ActionLink("Delete","Delete",new { id=items.RestaurantId})
            </td>
        </tr>
    }
</table>
